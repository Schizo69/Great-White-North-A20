<configs>
	<append xpath="/blocks">

                 <!-- 1X4 WALKWAYS -->
				 

<block name="NoRailWalkWay1x4A">
	<property name="Class" value="DrawBridge"/>
	<property name="CustomIcon" value="NoRailsWalkWay1"/>
	<property name="Material" value="MmetalDrawbridge"/>
	<property name="LightOpacity" value="1"/>
	<property name="Shape" value="ModelEntity"/>
	<property name="Texture" value="241"/>
	<property name="Model" value="#@modfolder:Resources/OpenWalkway.unity3d?NoRailsWalkWay1"/>
	<property name="ModelOffset" value="-2,0.5,0"/>
	<property name="DisplayType" value="blockMulti"/>
	<property name="MultiBlockDim" value="5,1,1"/>
	<property name="OnlySimpleRotations" value="false"/>
	<property name="ImposterExchange" value="imposterDontBlockHalf" param1="120"/>
	<property name="OpenSound" value="open_bridge"/>
	<property name="CloseSound" value="close_bridge"/>
	<property name="Place" value="DrawBridge"/>
	<property name="PlaceEverywhere" value="true"/>
	<property class="RepairItems">
		<property name="resourceForgedIron" value="100"/>
		<property name="resourceWood" value="800"/>
	</property>
	<drop event="Harvest" name="resourceScrapIron" count="400" tag="allHarvest"/>
	<drop event="Harvest" name="resourceWood" count="400" tag="allHarvest"/>
	<drop event="Destroy" count="0"/>
	<drop event="Harvest" name="terrStone" count="0" tool_category="Disassemble"/>
	<drop event="Destroy" name="resourceSpring" count="4,12" tag="salvageHarvest"/>
	<drop event="Destroy" name="resourceMechanicalParts" count="4,20" tag="salvageHarvest"/>
	<drop event="Fall" name="scrapMetalPile" count="1" prob="0.75" stick_chance="1"/>
	<property name="Stacknumber" value="1"/>
	<property name="DescriptionKey" value="drawBridgeGroupDesc"/>
	<property name="ActivationDistance" value="14"/> 
	<property name="Group" value="Building,advBuilding"/>
	<property name="FilterTags" value="fdecor,fdoors"/>
	<property name="SortOrder1" value="7060"/>
	<property name="SortOrder2" value="0600"/> 
</block>

