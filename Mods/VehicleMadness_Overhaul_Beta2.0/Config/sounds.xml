<configs>
<!--   Vehicle Madness Version 3 Beta 2.0 January 2022 A20 -->

<!--Reduce wheel bounce sounds -->
<set xpath="/Sounds/SoundDataNode[@name='vwheel_bounce']/MaxRepeatRate/@value">10</set>
<set xpath="/Sounds/SoundDataNode[@name='vwheel_bounce']/Noise[@ID='0']/@noise">5</set>
<!--Reduce wheel slide sounds -->
<set xpath="/Sounds/SoundDataNode[@name='vwheel_slide']/Noise[@ID='2']/@noise">5</set>
<set xpath="/Sounds/SoundDataNode[@name='vwheel_slide']/MaxRepeatRate/@value">.001</set>

